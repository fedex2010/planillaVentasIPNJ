//Dependencies
import React from 'react';
import { Route, Switch } from 'react-router-dom';

const AppRoutes = (items) => {
    let functionDeControl = () => {
        return true
    }
    
    return(
        <App>
            <Switch>
                <Route exact path="/" component={Welcome} />
                <Route exact path="/welcome" component={Welcome} />
                <Route exact path="/datos-basicos" component={DatosBasicos} />
                
                <ProtectedRouted exact functionDeControl={functionDeControl} 
                                    urlRedirect="/welcome" path="/pagina-de-prueba" 
                                    component={PaginaDePrueba} />

                <Route exact path="/error" component={Error} />
            </Switch>
        </App>
    )
}